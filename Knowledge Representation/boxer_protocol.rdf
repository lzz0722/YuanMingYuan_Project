  <rdf:Description rdf:about="http://example.org/boxer_protocol/Punishment_4">
    <rdf:type rdf:resource="http://schema.org/Event"/>
    <rdf:type rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E5_Event"/>
    <rdfs:label xml:lang="en">Punishment 4</rdfs:label>
    <schema1:description xml:lang="en">Kang-yi, Assistant Grand Secretary of State, President of the Board of Works, having lent his aid to the Boxers, serious disturbances broke out. He aided in publishing proclamations contrary to the treaties. A severe punishment was to have been inflicted on him at first, but he has died of disease. We order that the honors which he previously held shall be withdrawn from him and that he be at once degraded.</schema1:description>
    <schema1:isPartOf rdf:resource="http://example.org/boxer_protocol/ImperialEdict_1901_02_13"/>
    <schema1:actor rdf:resource="http://example.org/boxer_protocol/kang_yi"/>
    <crm:P11_had_participant rdf:resource="http://example.org/boxer_protocol/kang_yi"/>
  </rdf:Description>
